{
  "package" : "enedis.tic.service.config",
  "name": "TIC2WebSocketConfiguration",
  "type": "ConfigurationBase",
  "attributes": [
    {
      "name": "serverPort",
      "mandatory": true,
      "type": "Number",
      "constraint": "MinMax",
      "min": 1,
	  "max": 65535
    },
	{
      "name": "ticMode",
      "mandatory": false,
      "type": "Enum",
      "enumType": "TICMode"
    },
	{
      "name": "ticPortNames",
      "mandatory": false,
      "type": "List",
      "listType": "String",
      "constraint": "MinMaxSize",
      "min": 1
    }
  ]
}
